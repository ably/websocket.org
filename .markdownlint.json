      "div",
      "span",
      "a",
      "img",
      "br",
      "script",
      "style",
      "details",
      "summary",
      "sup",
      "sub",
      "AuthorBio",
      "SEOMetadata",
      "CardGrid",
      "Card",
      "LinkCard",
      "Tabs",
      "TabItem"
    ]
  },
  "MD034": false,
  "MD035": {
    "style": "---"
  },
  "MD036": true,
  "MD040": true,
  "MD041": true,
  "MD046": {
    "style": "fenced"
  },
  "MD047": true,
  "MD048": {
